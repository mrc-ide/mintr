{
         "metadata": {
              "x_col": "casesAvertedPer1000",
              "id_col": "intervention",
              "format": "long",
              "settings": ["irsUse", "netUse"]
          },
          "series": [
              {
                  "id": "none",
                  "y_formula": ["({population} / {procurePeoplePerNet}) * {priceNetStandard}"],
                  "name": "No intervention",
                  "type": "scatter",
                  "marker": {"color": "grey", "size": 10},
                  "error_x": {
                      "type": "data",
                      "width": 0,
                      "col": "casesAvertedPer1000ErrorPlus",
                      "colminus": "casesAvertedPer1000ErrorMinus"
                  }
              },
              {
                  "id": "ITN",
                  "y_formula": ["({population} / {procurePeoplePerNet}) * {priceNetStandard}  + ({priceDelivery} * {population} * (({procureBuffer} + 100)/ 100))"],
                  "name": "Pyrethoid ITN",
                  "type": "scatter",
                  "marker": {"color": "blue", "size": 10},
                  "error_x": {
                      "type": "data",
                      "width": 0,
                      "col": "casesAvertedPer1000ErrorPlus",
                      "colminus": "casesAvertedPer1000ErrorMinus"
                  }
              },
              {
                  "id": "PBO",
                  "y_formula": ["({population} / {procurePeoplePerNet}) * {priceNetPBO}  + ({priceDelivery} * {population} * (({procureBuffer} + 100)/ 100))"],
                  "name": "Switch to Pyrethoid-PBO ITN",
                  "type": "scatter",
                  "marker": {"color": "turquoise", "size": 10},
                  "error_x": {
                      "type": "data",
                      "width": 0,
                      "col": "casesAvertedPer1000ErrorPlus",
                      "colminus": "casesAvertedPer1000ErrorMinus"
                  }
              },
              {
                  "id": "IRS",
                  "y_formula": ["({population} / {procurePeoplePerNet}) * {priceNetStandard} + 3 * ({priceIRSPerPerson} * {population})"],
                  "name": "Only IRS",
                  "type": "scatter",
                  "marker": {"color": "purple", "size": 10},
                  "error_x": {
                      "type": "data",
                      "width": 0,
                      "col": "casesAvertedPer1000ErrorPlus",
                      "colminus": "casesAvertedPer1000ErrorMinus"
                  }
              },
              {
                  "id": "ITN-IRS",
                  "y_formula": ["({population} / {procurePeoplePerNet}) * {priceNetStandard} + ({priceDelivery} * {population} * (({procureBuffer} + 100) / 100)) + 3 * ({priceIRSPerPerson} * {population})"],
                  "name": "Add IRS to Pyrethoid ITN",
                  "type": "scatter",
                  "marker": {"color": "darkred", "size": 10},
                  "error_x": {
                      "type": "data",
                      "width": 0,
                      "col": "casesAvertedPer1000ErrorPlus",
                      "colminus": "casesAvertedPer1000ErrorMinus"
                  }
              },
              {
                  "id": "PBO-IRS",
                  "y_formula": ["({population} / {procurePeoplePerNet}) * {priceNetPBO} + ({priceDelivery} * {population} * (({procureBuffer} + 100) / 100)) + 3 * ({priceIRSPerPerson} * {population})"],
                  "name": "Add IRS to Pyrethoid-PBO ITN",
                  "type": "scatter",
                  "marker": {"color": "orange", "size": 10},
                  "error_x": {
                      "type": "data",
                      "width": 0,
                      "col": "casesAvertedPer1000ErrorPlus",
                      "colminus": "casesAvertedPer1000ErrorMinus"
                  }
              }
          ],
          "layout": {
              "title": "Strategy costs over 3 years vs Cases averted",
              "xaxis": {
                  "title": "Cases averted per 1,000 people across 3-years",
                  "showline": true,
                  "autorange": true,
                  "zeroline": false
              },
              "yaxis": {
                  "title": "Total costs ($10,000 USD)",
                  "showline": true,
                  "autorange": true,
                  "zeroline": false
              },
              "hoverlabel": {
                  "namelength": -1
              },
              "mintcustom": {
                  "hoverposition": "below"
              },
              "hovermode": "closest"
          }
}
