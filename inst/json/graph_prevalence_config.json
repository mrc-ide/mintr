{
    "metadata": {
          "x_col": "month",
          "y_col": "value",
          "id_col": "intervention",
          "format": "long",
          "settings": ["irsUse", "netUse"]
    },
    "layout": {
        "title": "Projected prevalence in under 5 year olds",
        "xaxis": {
            "title": "years of intervention",
            "showline": true,
            "tickvals": [12, 24, 36],
            "ticktext": [1, 2, 3]
        },
        "yaxis": {
            "title": "prevalence (%)",
            "showline": true,
            "autorange": true,
            "rangemode": "tozero",
            "tickformat": ".1%"
        }
    },
    "series": [
        {
            "id": "none",
            "name": "No intervention",
            "type": "lines",
            "marker": {"color": "grey"},
            "hovertemplate": "%{y:.2%}"
        },
        {
            "id": "llin",
            "name": "Pyrethroid LLIN only",
            "type": "lines",
            "marker": {"color": "blue"},
            "hovertemplate": "%{y:.2%}"
        },
        {
            "id": "irs",
            "name": "IRS only",
            "type": "lines",
            "marker": {"color": "purple"},
            "hovertemplate": "%{y:.2%}"
        },
        {
            "id": "irs-llin",
            "name": "Pyrethroid LLIN with IRS",
            "type": "lines",
            "marker": {"color": "darkred"},
            "hovertemplate": "%{y:.2%}"
        },
        {
            "id": "llin-pbo",
            "name": "Pyrethroid-PBO LLIN only",
            "type": "lines",
            "marker": {"color": "turquoise"},
            "hovertemplate": "%{y:.2%}"
        },
        {
            "id": "irs-llin-pbo",
            "name": "Pyrethroid-PBO LLIN with IRS",
            "type": "lines",
            "marker": {"color": "orange"},
            "hovertemplate": "%{y:.2%}"
        }
    ],
    "config": {
        "editable": true
    }
}