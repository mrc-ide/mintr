{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "string",
      "pattern": "^[0-9]{8}$"
    },
    "mintr": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
    }
  },
  "additionalProperties": false,
  "required": [
    "data",
    "mintr"
  ]
}
